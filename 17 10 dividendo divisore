#include <iostream>
using namespace std;
int main()
{
    int a, b;
    int quoz = 0;
    cout << " due numeri (dividendo e divisore):";
    cin >> a >> b;
    while (a >= b) {
        a -= b;
        quoz++;
    }
    cout << " quoziente =" << quoz << endl;
    cout << " resto =" << a << endl;
    return 0;
}
